        <activity android:name=".ui.checkBoxActivityNew">
        </activity>
        <activity android:name=".ui.RxDemoActivity">
        </activity>
        <activity
            android:name=".ui.WechatActivity"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.APP_ERROR"/>

                <category android:name="android.intent.category.DEFAULT"/>

                <data
                    android:host="192.168.1.1"
                    android:mimeType="text/plain"
                    android:pathPrefix="/hahah"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.ShortCutActivity">

            <!-- 支持3dTouch -->
            <meta-data
                android:name="android.app.shortcuts"
                android:resource="@xml/shortcuts"/>
