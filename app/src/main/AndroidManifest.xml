        <activity android:name=".ui.TouchDemoActivity">
        </activity>
        <activity android:name=".ui.TouchListviewActivity">
        </activity>
        <activity android:name=".ui.DemoScrollActivity">

        </activity>
        <activity android:name=".ui.AActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.BActivity">
