        <activity android:name=".ui.FileSelectorActivity">
        </activity>
        <activity android:name=".ui.IndexActivity">
        </activity>
        <activity android:name=".ui.ShareActionActivity">
        </activity>

        <service
            android:name=".services.MyIntentService"
            android:exported="false">
        </service>

        <receiver
            android:name=".broadcasts.MyReceiver"
            android:enabled="false"
            android:exported="false">
        </receiver>

        <activity android:name=".ui.IntentServiceActivity">
        </activity>
        <activity android:name=".ui.SimpleTestActivity">
        </activity>
        <activity android:name=".ui.SlidingMenuActivity">
        </activity>
        <activity android:name=".ui.TestScrollActivity">
        </activity>
        <activity android:name=".ui.RecyclerViewActivity">
